            android:lineSpacingExtra="18sp"
            android:text="Fashion And Designs"
            android:textColor="@color/colorAccent"
            android:textSize="18sp"
            android:textStyle="bold"
            android:typeface="serif"
            app:layout_constraintBottom_toBottomOf="@+id/fashion"
            app:layout_constraintEnd_toEndOf="@+id/fashion" />

        <ImageView
            android:id="@+id/mpesa"
            android:layout_width="284dp"
            android:layout_height="137dp"
            android:layout_marginBottom="48dp"
            android:layout_marginEnd="36dp"
            android:layout_marginRight="36dp"
            android:src="@drawable/moneyagentss"
            app:layout_constraintBottom_toTopOf="@+id/guideline11"
            app:layout_constraintEnd_toEndOf="parent" />

        <TextView
            android:id="@+id/textView14"
            android:layout_width="wrap_content"
            android:layout_height="31dp"
            android:layout_marginEnd="40dp"
            android:layout_marginRight="40dp"
            android:background="@android:color/darker_gray"
            android:text="Financial Services"
            android:textColor="@android:color/background_light"
            android:textSize="18sp"
            android:textStyle="bold"
            android:typeface="serif"
            app:layout_constraintBottom_toBottomOf="@+id/mpesa"
            app:layout_constraintEnd_toEndOf="@+id/mpesa" />

        <ImageView
            android:id="@+id/houses"
            android:layout_width="254dp"
            android:layout_height="125dp"
            android:layout_marginEnd="16dp"
            android:layout_marginRight="16dp"
            android:layout_marginTop="64dp"
            android:src="@drawable/appatments"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/guideline19" />

        <TextView
            android:id="@+id/textView15"
            android:layout_width="143dp"
            android:layout_height="28dp"
            android:layout_marginEnd="60dp"
            android:layout_marginRight="60dp"
            android:layout_marginTop="140dp"
            android:background="@android:color/darker_gray"
            android:text="Vacant Houses"
            android:textColor="@android:color/background_light"
            android:textSize="18sp"
            android:textStyle="bold"
            android:typeface="serif"
            app:layout_constraintEnd_toEndOf="@+id/houses"
            app:layout_constraintTop_toTopOf="@+id/guideline12" />

        <ImageView
            android:id="@+id/salon"
            android:layout_width="250dp"
            android:layout_height="121dp"
            android:layout_marginBottom="16dp"
            android:layout_marginEnd="36dp"
            android:layout_marginRight="36dp"
            android:src="@drawable/salon"
