            android:layout_marginRight="60dp"
            android:background="@android:color/darker_gray"
            android:text="Salon And Barbershop"
            android:textColor="@android:color/background_light"
            android:textSize="18sp"
            android:textStyle="bold"
            android:typeface="serif"
            app:layout_constraintBottom_toTopOf="@+id/fashion"
            app:layout_constraintEnd_toEndOf="parent" />

        <ImageView
            android:id="@+id/pharmacy"
            android:layout_width="258dp"
            android:layout_height="117dp"
            android:layout_marginLeft="16dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="36dp"

            android:src="@drawable/pharmacy"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView3"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="109dp"
            android:layout_height="20dp"
            android:layout_marginLeft="104dp"
            android:layout_marginStart="104dp"
            android:layout_marginTop="124dp"
            android:background="@android:color/darker_gray"
            android:text="Chemist"
            android:textColor="@android:color/background_light"
            android:textSize="18sp"
            android:textStyle="bold"
            android:typeface="serif"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/groceries" />

        <ImageView
            android:id="@+id/generalshop"
            android:layout_width="250dp"
            android:layout_height="120dp"
            android:layout_marginBottom="12dp"
